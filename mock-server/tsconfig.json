{
  "compilerOptions": {
    "noEmit": false,
    "target": "esnext",
    "module": "nodenext",
    "outDir": "../dist/server",
    "rootDir": "./",
    "skipLibCheck": true,
    "strict": true,
    "rewriteRelativeImportExtensions": true,
    "erasableSyntaxOnly": true,
    "verbatimModuleSyntax": true,
    "types": [
      "node"
    ]
  }
}
